<Window x:Class="WpfApp1.WindowLoad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="WindowLoad" Height="450" Width="800" Background="Black"  
        ContentRendered="Window_ContentRendered" >
    <Grid Margin="-2,0,2,0">
        <ProgressBar x:Name="ProgressLoad" HorizontalAlignment="Left" Height="19" Margin="231,188,0,0" VerticalAlignment="Top" Width="358" Foreground="#FF00DCFF" ValueChanged="ProgressLoad_ValueChanged"/>
        <TextBlock HorizontalAlignment="Left" Margin="231,137,0,0" TextWrapping="Wrap" Text="WarningList 0.5 Alpha" VerticalAlignment="Top" Width="358" Height="46" Foreground="White" FontFamily="Yu Gothic UI Semibold" FontSize="36" IsEnabled="False" />
        <Button Name="Button2" Content="Start" HorizontalAlignment="Left" Margin="323,207,0,0" VerticalAlignment="Top" Width="167" Click="Button_Click" Visibility="Hidden"/>
        <Button x:Name="Exiter" Content="Exit" HorizontalAlignment="Left" Margin="323,232,0,0" VerticalAlignment="Top" Width="167" Click="Exiter_Click" Visibility="Hidden"/>
    </Grid>
</Window>
